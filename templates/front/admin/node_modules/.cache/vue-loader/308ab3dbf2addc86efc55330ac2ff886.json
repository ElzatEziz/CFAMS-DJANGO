     <span>个人中心</span>\n            </template>\n            <el-menu-item\n              @click=\"menuHandler('updatePassword')\"\n              :index=\"1-1\"\n            >修改密码</el-menu-item>\n             <el-menu-item\n              @click=\"menuHandler('center')\"\n              :index=\"1-2\"\n            >个人信息</el-menu-item>\n          </el-submenu>\n          <el-submenu\n            v-for=\" (menu,index) in item.backMenu\"\n            v-bind:key=\"menu.menu\"\n            :index=\"index+2+''\"\n          >\n            <template slot=\"title\">\n              <span>{{menu.menu}}</span>\n            </template>\n            <el-menu-item\n              v-for=\" (child,sort) in menu.child\"\n              v-bind:key=\"sort\"\n              @click=\"menuHandler(child.tableName)\"\n              :index=\"(index+2)+'-'+sort\"\n            >{{child.menu}}</el-menu-item>\n          </el-submenu>\n        </el-menu>\n      </div>\n    </div>\n  </el-aside>\n</template>\n<script>\n\timport menu from \"@/utils/menu\";\n\texport default {\n\t\tdata() {\n\t\t\treturn {\n\t\t\t\tmenuList: [],\n\t\t\t\tdynamicMenuRoutes: [],\n\t\t\t\trole: \"\"\n\t\t\t};\n\t\t},\n\t\tmounted() {\n\t\t\tlet menus = menu.list();\n\t\t\tthis.menuList = menus;\n\t\t\tthis.role = this.$storage.get(\"role\");\n      console.log(this.menuList)\n      console.log(this.role)\n\t\t},\n\t\tmethods: {\n\t\t\tmenuHandler(name) {\n      let router = this.$router\n      name = '/'+name\n      // if (name==='home'){\n      //   router.push('/index')\n      // }\n      router.push(name)\n\t\t\t}\n      \n\t\t}\n\t};\n</script>\n<style lang=\"scss\" scoped>\n  .index-aside{\n    height: 100%;\n  }\n</style>\n"]}]}